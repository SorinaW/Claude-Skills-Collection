{
  "name": "n8n-connection",
  "version": "1.0.0",
  "description": "Claude Code skills for n8n workflow automation",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "test:unit": "vitest run --config tests/unit/vitest.config.ts",
    "test:e2e": "npx tsx scripts/run-e2e-tests.ts",
    "test:e2e:quick": "vitest run --config tests/e2e/vitest.config.ts",
    "test:e2e:watch": "vitest --config tests/e2e/vitest.config.ts",
    "test:e2e:ui": "vitest --config tests/e2e/vitest.config.ts --ui",
    "setup:test": "npx tsx tests/e2e/setup-n8n.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:test:up": "docker-compose -f docker-compose.test.yml up -d",
    "docker:test:down": "docker-compose -f docker-compose.test.yml down -v",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "n8n",
    "workflow",
    "automation",
    "claude"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^25.0.0",
    "@vitest/ui": "^4.0.15",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
