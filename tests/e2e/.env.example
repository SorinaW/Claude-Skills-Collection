# n8n E2E Test Environment
# Copy this file to .env.test and fill in your values
# DO NOT commit .env.test - it contains secrets

# n8n Instance URLs
N8N_BASE_URL=http://localhost:5679
N8N_API_URL=http://localhost:5679/api/v1
N8N_WEBHOOK_URL=http://localhost:5679/webhook

# Your n8n API Key (get from n8n Settings > API)
N8N_API_KEY=your-api-key-here

# Your n8n User ID (optional, for owner-specific tests)
N8N_USER_ID=your-user-id-here
